<div class=" container mt-4">
<div class="row row-cols-1 row-cols-md-4 g-4">

{{#each list_videos_model}}

  <div class="col">
    <div class="card">
      <img src={{this.img}} class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
        <p class="card-text">{{this.decription}}</p>
      </div>
    </div>
  </div>

{{/each}}

</div>
</div>